{
  "name": "payever/payever-integration-oxid",
  "description": "payever payment module",
  "type": "oxideshop-module",
  "keywords": [
    "oxid",
    "modules",
    "eShop",
    "payever",
    "payment",
    "Santander",
    "installments"
  ],
  "homepage": "https://getpayever.com/",
  "license": [
    "MIT"
  ],
  "extra": {
    "oxideshop": {
      "target-directory": "payever"
    }
  },
  "require": {
    "php": ">=5.4.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "payever/core-sdk-php": "2.1.0",
    "payever/plugins-sdk-php": "2.0.0",
    "payever/payments-sdk-php": "2.6.3",
    "payever/products-sdk-php": "2.0.0",
    "monolog/monolog": "^v1.23.0"
  },
  "require-dev": {
    "oxid-esales/testing-library": "^5.0"
  },
  "config": {
    "allow-plugins": {
      "oxid-esales/oxideshop-unified-namespace-generator": true
    }
  }
}
